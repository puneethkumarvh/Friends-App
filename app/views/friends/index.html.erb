

<h1>Friends</h1>
<table class="table table-striped table-hover">
<thead class="table-dark">
<tr>
<th>First Name</th>
<th>Last Name</th>
<th>Email</th>
<th>Phone</th>
<th>Twitter</th>
<th> </th>
</tr>
</thead>
<tbody>
<div id="friends">

  <% @friends.each do |friend| %>
    <tr>
    <td><%=  friend.first_name %></td>
    <td><%= friend.last_name %></td>
    <td><%= friend.email %></td>
    <td><%= friend.phone %></td>
    <td><%= friend.twitter %></td>
    <td>
    <p>
     <%= link_to "Show ", friend ,class:"btn btn-secondary"%>
    </p></td></tr>
  <% end %>
  
  
</div>
</tbody>

</table>

<%= link_to "New friend", new_friend_path %>
